algoritmo "CalculadoraPorcentagem"
var
  converter, porcentagem: real
inicio
  escreva ("Digite o valor que gostaria de fazer o cálculo: ")
  leia(converter)
  escreva("agora escreva a porcentagem que gostaria de calcular no valor original: ")
  leia (porcentagem)
  escreva("A porcentagem de " , converter , " é: " ,(converter*porcentagem)/100)

fimalgoritmo
