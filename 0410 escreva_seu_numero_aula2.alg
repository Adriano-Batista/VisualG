algoritmo "SomaNumeros"
var
   n1: inteiro
   n2: inteiro
   soma: inteiro
inicio
      escreva ("Digite seu primeiro número inteiro ")
      leia (n1)
      escreva ("digite seu segundo número inteiro ")
      leia (n2)
      soma <- (n1 + n2)
      escreva ("O resultado da soma é ", soma)
fimalgoritmo
